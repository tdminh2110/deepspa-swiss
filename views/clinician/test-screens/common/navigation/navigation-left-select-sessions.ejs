<button id="bt_select_session" type="button" class="btn btn-primary btn-lg btn-block dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
    Select a session
</button>
<div id="list_Of_Sessions" class="dropdown-menu btn-block">
	<button id="Create_Session" type="button" class="btn btn-info btn-lg btn-block">Create Session</button>
    <% for (let mysession of sessions) { %>
        <button class="dropdown-item btn btn-lg btn-block" id="S_<%= mysession.created_date %>-<%= mysession.created_number %>" type="button" value="<%= mysession.id %>">S_<%= mysession.created_date %>-<%= mysession.created_number %></button>
    <% } %>
</div>