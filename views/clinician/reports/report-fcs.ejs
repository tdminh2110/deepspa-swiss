<%- include('../includes/head.ejs') %>
<body>
    <%- include('../includes/navigation.ejs') %>
    <main>
        <div class="container">
            <div class="row">
                <div class="col-1">
                    <a href="/clinician/report-tests-effectues?idsession=<%= id_session %>"><img src="/images/icons/retour.png" alt="Edit" height="40" width="63"></a>
                </div>
                <div class="col-11">
                    <div class="row">
                        <div class="col-12">
                            <h5 class="font-weight-bold text-success">Patient: <%= patient_family_name %> <%= patient_name %></h5>
                        </div>                                                
                    </div>
                    <div class="row">
                        <div class="col-12">
                            <h5 class="font-weight-bold text-success">
                                Session <%= session_created_date.slice(0,2) %>/<%= session_created_date.slice(2,4) %>/<%= session_created_date.slice(4) %>
                            </h5>
                        </div>                                                
                    </div>
                    <div class="row">
                        <div class="col-12"><h5 class="font-weight-bold text-primary">Echelle FCS (SCF 4 items)</h5><hr /></div>                                                                        
                    </div>
                    <div class="row">
                        <div class="col-12">
                            <div class="row">
                                <div class="col-12"><h5>Score = <%= FCS_S1 + FCS_S2 + FCS_S3 + FCS_S4 %></h5></div>
                            </div>
                            <div class="row">
                                <div class="col-12"><h5>Mémoire = <%= FCS_S1 %></h5></div>
                            </div>
                            <div class="row">
                                <div class="col-12"><h5>Concentration = <%= FCS_S2 %></h5></div>
                            </div>
                            <div class="row">
                                <div class="col-12"><h5>Capacité Mentale = <%= FCS_S3 %></h5></div>
                            </div>
                            <div class="row">
                                <div class="col-12"><h5>Vitalité = <%= FCS_S4 %></h5></div>
                            </div>
                        </div>
                    </div>                    
                </div>
            </div>
        </div>
    </main>
</body>

<%- include('../includes/end.ejs') %>