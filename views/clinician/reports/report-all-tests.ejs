                        <div class="col-6">
                            <table class="table table-borderless">      
                            <tr>
                                <td><h5 class="font-weight-bold">Audio:</h5></td>                                
                            </tr>
                            <tr>
                                <td>
                                    <audio controls>
                                        <source src="<%= do80_path_audio %>" type="audio/webm">                                                
                                    </audio>
                                </td>                                
                            </tr>                            
                            </table>                            
                        </div>                                                
                    </div>
