<%- include('../includes/head.ejs') %>
<body>
    <%- include('../includes/navigation.ejs') %>
    <main>
        <div class="container">
            <div class="row">
                <div class="col-1">
                    <a href="/clinician/report-tests-effectues?idsession=<%= id_session %>"><img src="/images/icons/retour.png" alt="Edit" height="40" width="63"></a>
                </div>
                <div class="col-11">
                    <div class="row">
                        <div class="col-12">
                            <h5 class="font-weight-bold text-success">Patient: <%= patient_family_name %> <%= patient_name %></h5>
                        </div>                                                
                    </div>
                    <div class="row">
                        <div class="col-12">
                            <h5 class="font-weight-bold text-success">
                                Session <%= session_created_date.slice(0,2) %>/<%= session_created_date.slice(2,4) %>/<%= session_created_date.slice(4) %>
                            </h5>
                        </div>                                                
                    </div>
                    <% if (mmse_active == 1) { %>
                    <div class="row mt-3">
                        <div class="col-12"><h5 class="font-weight-bold text-primary">Tests: MMSE</h5><hr /></div>                                                                        
                    </div>
                    <div class="row">
                        <div class="col-6">
                            <table class="table table-borderless">      
                            <tr>
                                <td><h5 class="font-weight-bold">Total: <%= mmse_total %></h5></td>                                
                            </tr>
                            <tr>
                                <td><h5 class="font-weight-bold">Orientation temporelle: <%= mmse_orientation_temporelle %></h5></td>                                
                            </tr>
                            <tr>
                                <td><h5 class="font-weight-bold">Orientation spatiale: <%= mmse_orientation_spatiale %></h5></td>                                
                            </tr>
                            <tr>
                                <td><h5 class="font-weight-bold">Liste choisie: <%= mmse_list_number %></h5></td>                                
                            </tr>
                            <tr>
                                <td><h5 class="font-weight-bold">Apprentissage: <%= mmse_apprentissage %></h5></td>                                
                            </tr>
                            <tr>
                                <td><h5 class="font-weight-bold">Attention et calcul: <%= mmse_attention_et_calcul %></h5></td>                                
                            </tr>
                            <tr>
                                <td><h5 class="font-weight-bold">Rappel: <%= mmse_rappel %></h5></td>                                
                            </tr>
                            <tr>
                                <td><h5 class="font-weight-bold">Langage: <%= mmse_langage %></h5></td>                                
                            </tr>
                            <tr>
                                <td><h5 class="font-weight-bold">Praxies constructives: <%= mmse_praxies_constructives %></h5></td>                                
                            </tr>
                            </table>                            
                        </div>
                        <div class="col-6">
                            <table class="table table-borderless">      
                            <tr>
                                <td><h5 class="font-weight-bold">Commentaires:</h5></td>                                
                            </tr>
                            <tr>
                                <td>
                                    <table class="table table-bordered">
                                    <tr>
                                        <td><h6><%= mmse_commentaires %></h6></td>                                
                                    </tr>
                                    </table>
                                </td>                                
                            </tr>
                            <tr>
                                <td><h5 class="font-weight-bold">Video: <% if (mmse_path_video != "") { %> OUI <% } else { %> NON <% } %></h5></td>                                
                            </tr>
                            <tr>
                                <td class="text-center">
                                    <% if (mmse_path_video != "") { %>
                                    <video width="500" controls>
                                        <source src="<%= mmse_path_video %>" type="video/webm">                                                
                                    </video>
                                    <% } %>
                                </td>                                
                            </tr>                            
                            </table>                            
                        </div>
                    </div>
                    <% } %>
                    <% if (do80_active == 1) { %>
                    <div class="row mt-3">
                        <div class="col-12"><h5 class="font-weight-bold text-primary">Test DO80</h5><hr /></div>                                                                        
                    </div>
                    <div class="row">
                        <div class="col-6">
                            <table class="table table-borderless">      
                            <tr>
                                <td><h5 class="font-weight-bold">Score 1 = <%= do80_score_1 %> / 80</h5></td>                                
                            </tr>
                            <tr>
                                <td><h5 class="font-weight-bold">Score 2 = <%= do80_score_2 %> / 80</h5></td>                                
                            </tr>
