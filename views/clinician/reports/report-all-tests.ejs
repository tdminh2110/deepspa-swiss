                                <td><h5 class="font-weight-bold">Audio:</h5></td>                                
                            </tr>
                            <tr>
                                <td>
                                    <audio controls>
                                        <source src="<%= deno_lexis_path_audio %>" type="audio/webm">                                                
                                    </audio>
                                </td>                                
                            </tr>                            
                            </table>                            
                        </div>                                                
                    </div>
                    <% } %>
                    <% if (portes_active == 1) { %>
